<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		.button {
			font-weight: bold;
			margin-left: 20px;
			margin-bottom: 20px;
		}
		.heading{
  			font-size: 2em;
  			font-weight: bold;
  			color: #777777;
  			margin-top: 40px;
  			margin-left: 10px;
  			margin-bottom: 20px;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:Label styleName="{style.heading}">Runde definieren </g:Label>
		<div styleName="body">
		<g:HorizontalPanel>
			<g:Button styleName="{style.button}" ui:field="bPersonal" />
			<g:Button styleName="{style.button}" ui:field="bForschung" />
			<g:Button styleName="{style.button}" ui:field="bMarketing" />
 		</g:HorizontalPanel>
 		</div>
	</g:HTMLPanel>
</ui:UiBinder> 